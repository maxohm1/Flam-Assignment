{
  "name": "cv-processing-web-viewer",
  "version": "1.0.0",
  "description": "TypeScript web viewer for computer vision processing results from Android app",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "server": "node server.js",
    "dev": "npm run build && npm run server",
    "install-deps": "npm install express ws",
    "test-server": "curl http://localhost:8080/api/health",
    "clean": "rm -f viewer.js viewer.js.map"
  },
  "keywords": [
    "computer-vision",
    "android",
    "typescript",
    "opencv",
    "image-processing",
    "websocket",
    "rest-api"
  ],
  "author": "SE Intern Assignment",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.7.3"
  }
}
